{
  "name": "jmpsvr-backend",
  "version": "1.0.0",
  "description": "backend",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec babel-node --presets=@babel/preset-env -- src",
    "build": "babel src -s -D -d dist --presets=@babel/preset-env",
    "init": "node dist/init.js",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "jmpsvr/backend",
  "author": "JiJi <i@mmdjiji.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "axios": "^0.27.2",
    "compression": "^1.5.2",
    "express": "^4.13.3",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.8.0",
    "mqtt": "^4.3.7",
    "pg-promise": "^10.11.1",
    "resource-router-middleware": "^0.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "eslint": "^8.20.0",
    "nodemon": "^2.0.19"
  }
}
